{
  "name": "fob",
  "version": "0.0.4",
  "description": "Opinionated tool for building React/Redux universal/multi-platform apps",
  "main": "build/index.js",
  "engineStrict": true,
  "engines": {
    "node": ">= 6.10.x <7",
    "npm": ">=4.5.x <5",
    "yarn": "0.24.5"
  },
  "author": "tribou",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tribou/fob.git"
  },
  "bugs": {
    "url": "https://github.com/tribou/fob/issues"
  },
  "homepage": "https://github.com/tribou/fob#readme",
  "devDependencies": {
    "@types/debug": "^0.0.29",
    "@types/jest": "^19.2.3",
    "@types/node": "^6.0.73",
    "@webpack-blocks/webpack2": "^0.4.0",
    "jest": "^20.0.1",
    "jest-serializer-path": "^0.1.0",
    "rimraf": "^2.6.1",
    "tslint": "^4.5.1",
    "tslint-microsoft-contrib": "^4.0.1",
    "typescript": "^2.3.2"
  },
  "dependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "autoprefixer": "^6.4.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-3": "^6.11.0",
    "browser-sync": "^2.14.0",
    "browser-sync-webpack-plugin": "^1.1.2",
    "compression-webpack-plugin": "^0.4.0",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "debug": "^2.6.8",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "glob": "^7.1.2",
    "json-loader": "^0.5.4",
    "node-notifier": "^5.1.2",
    "node-notifier-cli": "^1.0.1",
    "nodemon": "^1.10.2",
    "offline-plugin": "^4.5.2",
    "postcss-calc": "^5.3.1",
    "postcss-loader": "^2.0.5",
    "precss": "^1.4.0",
    "shelljs": "^0.7.7",
    "tslib": "^1.7.0",
    "url-loader": "^0.5.7",
    "webpack-blocks-split-vendor": "^0.2.0",
    "webpack-node-externals": "^1.3.3",
    "yargs": "^8.0.1"
  },
  "files": [
    "build"
  ],
  "bin": "build/index.js",
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "rimraf build; tsc -p tsconfig.release.json",
    "watch": "rimraf build; tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish '{src,__tests__}/**/*.{ts,tsx}'",
    "prepublish": "npm run lint && npm run build",
    "postpublish": "git push --follow-tags",
    "pretest": "npm run lint",
    "start": "DEBUG=fob:* node ${npm_package_bin_fob}",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test-watch": "jest --watch",
    "yarn": "npm run --silent yarn-bin -- install --ignore-scripts",
    "yarn-bin": "node bin/yarn-${npm_package_engines_yarn}.js"
  }
}
